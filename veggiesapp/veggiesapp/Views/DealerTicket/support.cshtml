<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <ul class="nav nav-tabs" id="myTab2" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="tab-outline-one" data-toggle="tab" href="#Tickets" role="tab" aria-controls="home" aria-selected="true">Genrate Tickets</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="tab-outline-two" data-toggle="tab" href="#OpenTickets" role="tab" aria-controls="profile" aria-selected="false">Open Tickets</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="tab-outline-three" data-toggle="tab" href="#ClosedTickets" role="tab" aria-controls="contact" aria-selected="false">Closed Tickets</a>
                    </li>
                </ul>

                <br>
                <div class="tab-content">
                    <div class="tab-pane fade show active" id="Tickets">
                        <div class="row">
                            <div class="col-lg-6">
                                <div>
                                    <div style="display:none;" role="status" aria-hidden="true">
                                        <div id="loading" class="loadingimg" style="height: 100%; width: 100%;" align="center">
                                            <img src="../images/loader.gif">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Support For</label>
                                        <select name="ctl00$ContentPlaceHolder1$ddl_supportfor" class="form-control">
                                            <option selected="selected" value="">Support for</option>
                                            <option value="2">app not working</option>
                                            <option value="3">Wallet not working</option>
                                            <option value="4">Other</option>
                                        </select>
                                        <span id="ctl00_ContentPlaceHolder1_RequiredFieldValidator13" style="display:none;"></span>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Subject</label>
                                        <input name="ctl00$ContentPlaceHolder1$tbx_Subject" type="text" maxlength="100" class="form-control" placeholder="Subject">
                                        <span id="ctl00_ContentPlaceHolder1_RequiredFieldValidator14" style="display:none;"></span>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">Comments</label>
                                        <textarea name="ctl00$ContentPlaceHolder1$tbx_Comments" rows="2" cols="20" class="form-control" placeholder="Subject"></textarea>
                                        <span style="display:none;"></span>
                                    </div>
                                    <input type="submit" name="ctl00$ContentPlaceHolder1$btnSubmit" value="Submit" id="ctl00_ContentPlaceHolder1_btnSubmit" class="btn btn-success">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="OpenTickets">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="dataTable_wrapper">
                                            <div>
                                                <div style="display:none;" role="status" aria-hidden="true">
                                                    <div id="loading" class="loadingimg" style="height: 100%; width: 100%;" align="center">
                                                        <img src="../images/loader.gif">
                                                    </div>
                                                </div>
                                                <table class="table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th class="header">Ticket ID</th>
                                                            <th class="header">Subject</th>
                                                            <th class="header">Status</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                                <div class="text-center">No tickets to display</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="ClosedTickets">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="dataTable_wrapper">
                                            <div>

                                                <div style="display:none;" role="status" aria-hidden="true">

                                                    <div id="loading" class="loadingimg" style="height: 100%; width: 100%;" align="center">
                                                        <img src="../images/loader.gif">
                                                    </div>

                                                </div>
                                                <table class="table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th class="header">Ticket ID</th>
                                                            <th class="header">Subject</th>
                                                            <th class="header">Status</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                                <div class="text-center">No tickets to display</div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>